<html>

<head>
  <meta charset="utf-8">
  <title>CKEditor</title>
  <link rel="stylesheet" href="css/style.css">
  <script src="https://cdn.ckeditor.com/ckeditor5/31.0.0/classic/ckeditor.js"></script>
</head>

<body>
  <div class="container" id="editor">This is some sample content.</div>
  <script>
    ClassicEditor
      .create(document.querySelector('#editor'))
      .then(editor => {
        console.log(editor);
      })
      .catch(error => {
        console.error(error);
      });
  </script>
</body>

</html>